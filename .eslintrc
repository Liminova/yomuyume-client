{
    "root": true,
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser"
    },
    "extends": [
        "plugin:vue/recommended",
        "plugin:prettier-vue/recommended",
        "eslint:recommended",
        "@vue/typescript/recommended",
        "prettier"
    ],
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "vue/no-multiple-template-root": "off"
    }
}